{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "gutenify/content-toggle-item",
  "category": "gutenify",
  "title": "Content toggle item",
  "version": "2",
  "attributes": {
    "blockClientId": {
      "type": "string",
      "default": ""
    },
    "blockAdvanceOptions": {
      "type": "object",
      "default": {
        "backgroundColor": "",
        "backgroundGradient": "",
        "padding": {
          "desktop": {}
        }
      }
    },
    "title": {
      "type": "string"
    },
    "initialOpen": {
      "type": "boolean",
      "default": true
    }
  },
  "supports": {
    "html": false
  },
  "editorScript": [
    "gutenify--content-toggle-item"
  ],
  "editorStyle": [
    "gutenify--content-toggle-item--editor"
  ],
  "style": [
    "gutenify--content-toggle-item--frontend"
  ]
}
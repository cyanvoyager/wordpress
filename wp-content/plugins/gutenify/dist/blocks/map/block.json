{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "gutenify/map",
  "category": "gutenify",
  "title": "Map",
  "version": "2",
  "attributes": {
    "blockClientId": {
      "type": "string",
      "default": ""
    },
    "blockAdvanceOptions": {
      "type": "object",
      "default": {}
    },
    "isSaved": {
      "type": "boolean",
      "default": false
    },
    "location": {
      "type": "string",
      "default": ""
    },
    "height": {
      "type": "number",
      "default": 400
    },
    "pinned": {
      "type": "boolean",
      "default": false
    },
    "zoom": {
      "type": "number",
      "default": 12
    },
    "language": {
      "type": "string",
      "default": "en"
    }
  },
  "editorScript": [
    "gutenify--map"
  ],
  "editorStyle": [
    "gutenify--map--editor"
  ],
  "style": [
    "gutenify--map--frontend"
  ]
}